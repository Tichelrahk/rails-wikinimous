<nav class="navbar bg-light sticky">
  <%= link_to 'Wiki Home', root_path, class: 'nav-item nav-link text-muted' %>
  <%= link_to 'Create New', new_article_path, class: 'nav-item nav-link text-muted' %>
</nav>
<div class="container bg-light">
  <h1><%= @article.title %></h1>
  <p><%= @article.content %></p>
  <%= link_to 'Back', root_path %>
  <p><%= link_to 'Edit', edit_article_path(@article) %> | <%= link_to 'Delete', article_path(@article), method: :delete, data: {confirm: "Are you sure?"} %></p>
</div>
